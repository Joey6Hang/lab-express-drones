<h2>Available drones</h2>
<ul>
    {{#each drones}}
    <li>
    <h2>{{this.name}}</h2>
    <p>propellers:{{this.propellers}}</p>
    <p>maxSpeed:{{this.maxSpeed}}</p>
    <a href="/drones/{{this._id}}/edit">edit</a>
    <a href="/drones/{{this._id}}/delete">delete</a>
    </li>
    {{/each}}
</ul>

